{
  "name": "mealodic",
  "version": "0.0.1",
  "private": true,
  "description": "Meal planning for individuals and families",
  "scripts": {
    "dev": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "dev:down": "docker compose -f docker-compose.yml -f docker-compose.dev.yml down",
    "dev:clean": "docker compose -f docker-compose.yml -f docker-compose.dev.yml down -v"
  },
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=22"
  }
}
